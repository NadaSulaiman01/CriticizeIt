=> {\n      const respond = await axios.get(\"https://en.wikipedia.org/w/api.php\", {\n        params: {\n          action: \"query\",\n          list: \"search\",\n          origin: \"*\",\n          format: \"json\",\n          srsearch: searchWord,\n        },\n      });\n      setResult(respond.data.query.search);\n    };\n\n    if (!result.length) {\n      if (searchWord) {\n        search();\n      }\n      console.log(\"from if\");\n    } else {\n      const debounceSearch = setTimeout(() => {\n        if (searchWord) {\n          search();\n        }\n        console.log(\"from else\");\n      }, 1500);\n\n      // Clean up\n      return () => {\n        clearTimeout(debounceSearch);\n      };\n    }\n  }, [searchWord, result.length]);\n\n  const filterHandler = (event) => {\n    const wordEntered = event.target.value;\n    setSearchWord(wordEntered);\n    const filteredCompanies = companies.companies.filter((company) => {\n      return company.name\n        .toLocaleLowerCase()\n        .includes(wordEntered.toLocaleLowerCase());\n    });\n    if (wordEntered === \"\") {\n      setFilteredData([]);\n    } else {\n      setFilteredData(filteredCompanies);\n    }\n  };\n\n  const clearInputHandler = () => {\n    setFilteredData([]);\n    setSearchWord(\"\");\n  };\n\n  return (\n    <div className={styles.searchSection}>\n      <p className=\"heading-secondary sm-margin\">Company searching made easy</p>\n      <div className={styles.searchContainer}>\n        <SearchIcon styling={styles.searchIcon} />\n        <input\n          type=\"text\"\n          name=\"company\"\n          className={styles.input}\n          placeholder=\"Search for a company\"\n          value={searchWord}\n          onChange={filterHandler}\n        />\n        {searchWord.length !== 0 && (\n          <div className={styles.closeIconContainer}>\n            <CloseIcon\n              clearInput={clearInputHandler}\n              styling={styles.closeIcon}\n            />\n          </div>\n        )}\n        {/* <button className=\"main-btn\">Search</button> */}\n      </div>\n      {filteredData.length !== 0 && (\n        <div className={styles.dataResults}>\n          {filteredData.slice(0, 15).map((company) => {\n            return (\n              <a\n                href=\"#\"\n                target=\"_blank\"\n                className={styles.companyLink}\n                key={company.id}\n              >\n                <div>{company.name}</div>\n              </a>\n            );\n          })}\n        </div>\n      )}\n    </div>\n  );\n}\n\nexport default Search;\n",[],"/home/esraa/CSE/learning/learn_ReactJS/kimz code/test-grad/src/components/Nav/Nav.js",[],[],"/home/esraa/CSE/learning/learn_ReactJS/kimz code/test-grad/src/components/Hero/Hero.js",[],[],"/home/esraa/CSE/learning/learn_ReactJS/kimz code/test-grad/src/components/Logo/Logo.js",[],[],"/home/esraa/CSE/learning/learn_ReactJS/kimz code/test-grad/src/components/AddCompany/AddCompany.js",[],[],"/home/esraa/CSE/learning/learn_ReactJS/kimz code/test-grad/src/App/App.js",[],[],"/home/esraa/CSE/learning/learn_ReactJS/kimz code/test-grad/src/svg/SearchIcon.js",[],[],"/home/esraa/CSE/learning/learn_ReactJS/kimz code/test-grad/src/svg/CloseIcon.js",[],[],"/home/esraa/CSE/learning/learn_ReactJS/kimz code/test-grad/src/pages/Home/Home.js",[],[],"/home/esraa/CSE/learning/learn_ReactJS/kimz code/test-grad/src/pages/SignUp/SignUp.js",[],[],"/home/esraa/CSE/learning/learn_ReactJS/kimz code/test-grad/src/pages/Login/Login.js",[],[],"/home/esraa/CSE/learning/learn_ReactJS/kimz code/test-grad/src/pages/Root/Root.js",[],[],"/home/esraa/CSE/learning/learn_ReactJS/kimz code/test-grad/src/components/Header/Header.js",[],[],"/home/esraa/CSE/learning/learn_ReactJS/kimz code/test-grad/src/pages/ErrorPage/ErrorPage.js",[],[],"/home/esraa/CSE/learning/learn_ReactJS/kimz code/test-grad/src/pages/Companies/Companies.js",[],[],"/home/esraa/CSE/learning/learn_ReactJS/kimz code/test-grad/src/pages/Sala